package statistics_test;

import static org.junit.Assert.*;

import org.junit.Before;
import org.junit.Test;

import statistics.DataFrame;

public class TestBasicTest {

	@Test
	public void test() {
		DataFrame dataFrame = new DataFrame(3,2);
		dataFrame.setAt(1, 1, 1.0);
		dataFrame.setAt(2, 1, 2.0);
		dataFrame.setAt(3, 2, 3.0);
		dataFrame.setAt(1, 2, 4.0);
		dataFrame.setAt(2, 2, 5.0);
		dataFrame.setAt(3, 2, 6.0);
		dataFrame.setHeader(1, "Column 1");
		dataFrame.setHeader(2, "Column 2");
		dataFrame.setHeader(3, "Values");
		
		dataFrame.upperTriangular();
		
		dataFrame.show();
	}

}
